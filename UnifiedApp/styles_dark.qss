/* ============================================
   ANALYTICS DASHBOARD STYLING - DARK THEME
   ============================================ */

/* Main Dashboard Window */
QMainWindow#AnalyticsDashboard {
    background: qlineargradient(x1:0, y1:0, x2:1, y2:1,
        stop:0 #0a0c12, stop:0.3 #12141a, stop:0.6 #1a1d28, stop:1 #1e2130);
}

/* Dashboard Scroll Area */
QScrollArea#dashboardScrollArea {
    background: transparent;
    border: none;
}

/* Dashboard Title */
QLabel#dashboardTitle {
    color: #4DD0E1;
    font-size: 16px;
    font-weight: 700;
    letter-spacing: 1px;
    background: transparent;
    padding: 4px 0px;
}

/* Dashboard Filter Label */
QLabel#filterLabel {
    color: #9aa0a6;
    font-size: 10px;
    font-weight: 600;
    letter-spacing: 0.5px;
    background: transparent;
}

/* Dashboard ComboBoxes */
QComboBox#dashboardCombo {
    background: rgba(36, 39, 46, 0.9);
    color: #c4c7cc;
    border: 1px solid rgba(0, 188, 212, 0.2);
    border-radius: 6px;
    padding: 6px 12px;
    font-size: 11px;
    font-weight: 500;
    letter-spacing: 0.3px;
    min-height: 28px;
}

QComboBox#dashboardCombo:hover {
    background: rgba(45, 49, 59, 0.95);
    border: 1px solid rgba(0, 188, 212, 0.35);
}

QComboBox#dashboardCombo:focus {
    border: 2px solid #00BCD4;
}

QComboBox#dashboardCombo::drop-down {
    border: none;
    width: 30px;
    subcontrol-origin: padding;
    subcontrol-position: center right;
    padding-right: 8px;
}

QComboBox#dashboardCombo::down-arrow {
    image: none;
    border-left: 5px solid transparent;
    border-right: 5px solid transparent;
    border-top: 6px solid #00BCD4;
    width: 0;
    height: 0;
}

QComboBox#dashboardCombo QAbstractItemView {
    background: rgba(28, 30, 38, 0.98);
    color: #e8eaed;
    border: 1px solid rgba(0, 188, 212, 0.35);
    border-radius: 8px;
    selection-background-color: rgba(0, 188, 212, 0.25);
    selection-color: #ffffff;
    padding: 6px;
    outline: none;
}

QComboBox#dashboardCombo QAbstractItemView::item {
    padding: 10px 14px;
    border-radius: 4px;
    margin: 2px;
}

QComboBox#dashboardCombo QAbstractItemView::item:hover {
    background: rgba(0, 188, 212, 0.18);
    color: #ffffff;
}

/* Dashboard Buttons */
QPushButton#dashboardRefreshBtn,
QPushButton#dashboardExportBtn {
    background: rgba(0, 137, 123, 0.85);
    color: #ffffff;
    border: 1px solid rgba(0, 188, 212, 0.25);
    border-radius: 6px;
    padding: 6px 16px;
    font-size: 10px;
    font-weight: 600;
    letter-spacing: 0.5px;
    min-height: 28px;
}

QPushButton#dashboardRefreshBtn:hover,
QPushButton#dashboardExportBtn:hover {
    background: rgba(0, 150, 136, 0.95);
    border: 1px solid rgba(0, 188, 212, 0.4);
}

QPushButton#dashboardRefreshBtn:pressed,
QPushButton#dashboardExportBtn:pressed {
    background: rgba(0, 105, 92, 0.95);
    border: 1px solid rgba(0, 188, 212, 0.6);
    transform: scale(0.98);
}

