/* ============================================
   RADAR SYSTEM v3.0 â€” DARK THEME
   Glass-morphism inspired with teal/cyan accents
   Global Font: Inter / Segoe UI Variable (latest)
   ============================================ */

/* ============================================
   GLOBAL DEFAULTS
   ============================================ */

QWidget {
    font-family: "Inter", "Segoe UI Variable", "SF Pro Display", "Segoe UI", "Roboto", "Helvetica Neue", sans-serif;
    font-size: 13px;
}

QToolTip {
    background-color: #24272e;
    color: #e8eaed;
    border: 1px solid rgba(255, 255, 255, 0.08);
    border-radius: 6px;
    padding: 8px 12px;
    font-size: 13px;
}

/* ============================================
   LOGIN DIALOG
   ============================================ */

QDialog#LoginDialog {
    background: qlineargradient(x1:0, y1:0, x2:1, y2:1,
        stop:0 #0a0c12, stop:0.3 #151720, stop:0.6 #1a1d28, stop:1 #1e2130);
    border: none;
}

QWidget#centerContainer {
    background: rgba(24, 27, 33, 0.85);
    border: 1px solid rgba(0, 188, 212, 0.2);
    border-radius: 16px;
}

QLabel#titleLabel {
    color: #e8eaed;
    font-size: 42px;
    font-weight: 800;
    padding: 10px 0px;
    background: transparent;
    letter-spacing: 3px;
}

QLabel#subtitleLabel {
    color: #00BCD4;
    font-size: 20px;
    font-weight: 600;
    padding: 4px 0px;
    background: transparent;
    letter-spacing: 6px;
}

QLabel#welcomeLabel {
    color: #4DD0E1;
    font-size: 18px;
    font-weight: 500;
    padding: 4px 0px;
    background: transparent;
    letter-spacing: 2px;
}

QFrame#inputFrame {
    background: rgba(18, 20, 26, 0.9);
    border: 1px solid rgba(0, 188, 212, 0.15);
    border-radius: 14px;
    padding: 18px;
}

QDialog#LoginDialog QLineEdit {
    background-color: rgba(12, 14, 20, 0.95);
    border: 1px solid rgba(0, 188, 212, 0.2);
    border-radius: 10px;
    padding: 14px 20px;
    font-size: 16px;
    color: #e8eaed;
    selection-background-color: #00897B;
    selection-color: white;
    min-height: 50px;
}

QDialog#LoginDialog QLineEdit:focus {
    background-color: rgba(12, 14, 20, 1);
    border: 2px solid #00BCD4;
    box-shadow: 0 0 10px rgba(0, 188, 212, 0.3);
}

QDialog#LoginDialog QLineEdit:hover {
    background-color: rgba(18, 20, 26, 0.95);
    border: 1px solid rgba(0, 188, 212, 0.4);
}

QDialog#LoginDialog QLineEdit::placeholder {
    color: #5f6368;
    font-style: italic;
}

QLabel#fieldLabel {
    color: #00BCD4;
    font-size: 13px;
    font-weight: 700;
    text-transform: uppercase;
    letter-spacing: 2px;
    padding: 0px 4px 8px 4px;
    min-height: 24px;
}

/* Primary Login Button */
QPushButton#loginButton {
    background: qlineargradient(x1:0, y1:0, x2:1, y2:0,
        stop:0 #00897B, stop:1 #00BCD4);
    color: #ffffff;
    border: none;
    border-radius: 12px;
    padding: 16px 48px;
    font-size: 16px;
    font-weight: 700;
    min-width: 160px;
    min-height: 56px;
    letter-spacing: 2px;
    text-transform: uppercase;
}

QPushButton#loginButton:hover {
    background: qlineargradient(x1:0, y1:0, x2:1, y2:0,
        stop:0 #00ACC1, stop:1 #26C6DA);
}

QPushButton#loginButton:pressed {
    background: #00796B;
}

QPushButton#loginButton:disabled {
    background: #2d313b;
    color: #5f6368;
}

QPushButton#cancelButton {
    background: rgba(45, 49, 59, 0.7);
    color: #c4c7cc;
    border: 1px solid rgba(255, 255, 255, 0.12);
    border-radius: 12px;
    padding: 16px 48px;
    font-size: 16px;
    font-weight: 700;
    min-width: 160px;
    min-height: 56px;
    letter-spacing: 2px;
    text-transform: uppercase;
}

QPushButton#cancelButton:hover {
    background: rgba(52, 56, 63, 0.9);
    border: 1px solid rgba(0, 188, 212, 0.3);
    color: #e8eaed;
}

QLabel#errorLabel {
    color: #ef5350;
    font-size: 14px;
    font-weight: 500;
    padding: 12px 16px;
    background: rgba(198, 40, 40, 0.1);
    border-radius: 8px;
    border-left: 3px solid #c62828;
}

QLabel#successLabel {
    color: #4CAF50;
    font-size: 14px;
    font-weight: 500;
    padding: 12px 16px;
    background: rgba(46, 125, 50, 0.1);
    border-radius: 8px;
    border-left: 3px solid #2e7d32;
}

QLabel#hintLabel {
    color: #5f6368;
    font-size: 13px;
    padding: 10px;
    background: transparent;
}

QFrame#infoPanel {
    background: rgba(0, 188, 212, 0.06);
    border-left: 3px solid #00BCD4;
    border-radius: 8px;
    padding: 12px;
}

QLabel#infoPanelText {
    color: #9aa0a6;
    font-size: 13px;
    line-height: 1.6;
}

QFrame#divider {
    background: rgba(255, 255, 255, 0.06);
    max-height: 1px;
}

QLabel#footerLabel {
    color: #5f6368;
    font-size: 12px;
    padding: 12px;
    background: transparent;
    letter-spacing: 1.5px;
    font-weight: 500;
}

QCheckBox {
    color: #9aa0a6;
    font-size: 14px;
    spacing: 8px;
}

QCheckBox::indicator {
    width: 20px;
    height: 20px;
    border-radius: 4px;
    border: 1px solid rgba(255, 255, 255, 0.12);
    background: rgba(45, 49, 59, 0.8);
}

QCheckBox::indicator:hover {
    border: 1px solid #00BCD4;
}

QCheckBox::indicator:checked {
    background: #00BCD4;
    border: 1px solid #0097A7;
}

QLineEdit#usernameInput {
    background-color: rgba(28, 30, 38, 0.9);
    border: 1px solid rgba(255, 255, 255, 0.08);
    border-radius: 8px;
    padding: 12px 16px;
    font-size: 15px;
    color: #e8eaed;
    selection-background-color: #00897B;
    selection-color: white;
    min-height: 36px;
}

QLineEdit#usernameInput:focus {
    background-color: rgba(28, 30, 38, 0.95);
    border: 1px solid #00BCD4;
}

QLineEdit#usernameInput:hover {
    background-color: rgba(36, 39, 46, 0.9);
    border: 1px solid rgba(255, 255, 255, 0.12);
}

QFrame#passwordContainer {
    background-color: rgba(12, 14, 20, 0.95);
    border: 1px solid rgba(0, 188, 212, 0.2);
    border-radius: 10px;
    min-height: 50px;
}

QFrame#passwordContainer:focus-within {
    border: 2px solid #00BCD4;
}

QLineEdit#passwordInput {
    background-color: transparent;
    border: none;
    border-right: 1px solid rgba(0, 188, 212, 0.15);
    border-radius: 0px;
    padding: 14px 20px;
    font-size: 16px;
    color: #e8eaed;
    selection-background-color: #00897B;
    selection-color: white;
    min-height: 50px;
}

QLineEdit#passwordInput:focus {
    background-color: transparent;
    border: none;
    border-right: 1px solid rgba(0, 188, 212, 0.3);
}

QPushButton#togglePassword {
    background: transparent;
    border: none;
    padding: 8px 12px;
    margin: 0px;
    color: #00BCD4;
    font-size: 12px;
    font-weight: 700;
    letter-spacing: 1px;
    min-width: 80px;
    min-height: 50px;
}

QPushButton#togglePassword:hover {
    color: #4DD0E1;
    background: rgba(0, 188, 212, 0.12);
}

/* Theme toggle on Login */
QPushButton#themeToggleLogin {
    background: rgba(24, 27, 33, 0.7);
    color: #c4c7cc;
    border: 1px solid rgba(0, 188, 212, 0.2);
    border-radius: 8px;
    padding: 10px 20px;
    font-size: 13px;
    font-weight: 700;
    letter-spacing: 1px;
}

QPushButton#themeToggleLogin:hover {
    background: rgba(0, 188, 212, 0.15);
    color: #00BCD4;
    border: 1px solid rgba(0, 188, 212, 0.4);
}

/* ============================================
   MAIN WINDOW
   ============================================ */

QMainWindow {
    background: #12141a;
}

QWidget#centralWidget {
    background: transparent;
}

/* Toolbar */
QToolBar#mainToolbar {
    background: rgba(24, 27, 33, 0.95);
    border: none;
    border-bottom: 1px solid rgba(255, 255, 255, 0.04);
    padding: 6px 10px;
    spacing: 6px;
}

QToolBar::separator {
    background: rgba(255, 255, 255, 0.06);
    width: 1px;
    margin: 2px 6px;
}

/* User Label */
QLabel#userLabel {
    color: #e8eaed;
    font-size: 13px;
    font-weight: 600;
    letter-spacing: 1px;
    padding: 6px 14px;
    background: rgba(0, 188, 212, 0.08);
    border-radius: 6px;
    border: 1px solid rgba(0, 188, 212, 0.15);
}

/* Panels */
QWidget#leftPanel, QWidget#rightPanel, QWidget#centerPanel {
    background: rgba(24, 27, 33, 0.85);
    border: 1px solid rgba(255, 255, 255, 0.04);
    border-radius: 8px;
    padding: 6px;
}

/* Component List */
QListWidget#componentList {
    background: rgba(18, 20, 26, 0.9);
    color: #c4c7cc;
    border: 1px solid rgba(255, 255, 255, 0.04);
    border-radius: 8px;
    padding: 6px;
    font-size: 14px;
    outline: none;
}

QListWidget#componentList::item {
    background: rgba(36, 39, 46, 0.7);
    border: 1px solid rgba(255, 255, 255, 0.04);
    border-radius: 6px;
    padding: 2px;
    margin: 3px 0px;
    color: #c4c7cc;
}

QListWidget#componentList::item:hover {
    background: rgba(45, 49, 59, 0.9);
    border: 1px solid rgba(0, 188, 212, 0.2);
}

QListWidget#componentList::item:selected {
    background: rgba(0, 188, 212, 0.15);
    border: 1px solid rgba(0, 188, 212, 0.3);
    color: #ffffff;
}

/* Canvas */
QGraphicsView#mainCanvas {
    background: #0e1015;
    border: 1px solid rgba(255, 255, 255, 0.04);
    border-radius: 8px;
}

QGraphicsView#mainCanvas:focus {
    border: 1px solid rgba(0, 188, 212, 0.3);
}

/* Analytics Panel */
QTextEdit#analyticsPanel {
    background: rgba(18, 20, 26, 0.9);
    color: #c4c7cc;
    border: 1px solid rgba(255, 255, 255, 0.04);
    border-radius: 6px;
    padding: 8px;
    font-family: "JetBrains Mono", "Fira Code", "Consolas", "Monaco", "Courier New", monospace;
    font-size: 13px;
    selection-background-color: #00897B;
    selection-color: white;
}

/* Status Label */
QLabel#statusLabel {
    color: #4CAF50;
    font-size: 13px;
    font-weight: 500;
    padding: 6px 14px;
    background: rgba(46, 125, 50, 0.08);
    border-radius: 6px;
    border-left: 2px solid #2e7d32;
    letter-spacing: 0.5px;
}

/* Component Label Header */
QLabel#componentsLabel {
    color: #e8eaed;
    font-size: 13px;
    font-weight: 700;
    text-transform: uppercase;
    letter-spacing: 2px;
    padding: 10px 12px;
    background: rgba(0, 188, 212, 0.06);
    border-radius: 8px;
    border-left: 3px solid #00BCD4;
}

/* Count Label */
QLabel#countLabel {
    color: #5f6368;
    font-size: 12px;
    padding: 4px 10px;
    letter-spacing: 0.5px;
}

/* Heading Labels */
QLabel[heading="true"] {
    color: #e8eaed;
    font-size: 15px;
    font-weight: 600;
    text-transform: uppercase;
    letter-spacing: 2px;
    padding: 6px 0px;
    background: transparent;
}

QLabel[hint="true"] {
    color: #5f6368;
    font-size: 12px;
    padding: 6px;
    background: transparent;
    letter-spacing: 0.3px;
}

/* Toolbar Buttons */
QPushButton#saveButton, QPushButton#loadButton, QPushButton#clearButton {
    background: rgba(0, 137, 123, 0.9);
    color: #ffffff;
    border: none;
    border-radius: 6px;
    padding: 8px 20px;
    font-size: 12px;
    font-weight: 600;
    text-transform: uppercase;
    letter-spacing: 1px;
    min-height: 32px;
}

QPushButton#saveButton:hover, QPushButton#loadButton:hover, QPushButton#clearButton:hover {
    background: rgba(0, 150, 136, 0.95);
}

QPushButton#saveButton:pressed, QPushButton#loadButton:pressed, QPushButton#clearButton:pressed {
    background: #00695C;
}

/* Add Component Type Button */
QPushButton#addTypeButton {
    background: qlineargradient(x1:0, y1:0, x2:1, y2:0,
        stop:0 #00BCD4, stop:1 #00ACC1);
    color: #ffffff;
    border: none;
    border-radius: 6px;
    padding: 8px 20px;
    font-size: 12px;
    font-weight: 700;
    text-transform: uppercase;
    letter-spacing: 1px;
    min-height: 32px;
}

QPushButton#addTypeButton:hover {
    background: qlineargradient(x1:0, y1:0, x2:1, y2:0,
        stop:0 #26C6DA, stop:1 #4DD0E1);
}

QPushButton#addTypeButton:pressed {
    background: #0097A7;
}

/* Inline Add Button */
QPushButton#addInlineButton {
    background: rgba(0, 188, 212, 0.08);
    color: #00BCD4;
    border: 1px dashed rgba(0, 188, 212, 0.3);
    border-radius: 8px;
    padding: 10px;
    font-size: 13px;
    font-weight: 600;
    letter-spacing: 0.5px;
}

QPushButton#addInlineButton:hover {
    background: rgba(0, 188, 212, 0.12);
    border: 1px dashed rgba(0, 188, 212, 0.5);
    color: #4DD0E1;
}

QPushButton#addInlineButton:pressed {
    background: rgba(0, 188, 212, 0.18);
}

/* Theme Toggle Button (toolbar) */
QPushButton#themeToggleBtn {
    background: rgba(45, 49, 59, 0.8);
    color: #c4c7cc;
    border: 1px solid rgba(255, 255, 255, 0.08);
    border-radius: 6px;
    padding: 8px 18px;
    font-size: 12px;
    font-weight: 600;
    text-transform: uppercase;
    letter-spacing: 1px;
    min-height: 32px;
}

QPushButton#themeToggleBtn:hover {
    background: rgba(52, 56, 63, 0.9);
    border: 1px solid rgba(0, 188, 212, 0.25);
    color: #e8eaed;
}

/* Logout Button */
QPushButton#logoutButton {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1,
        stop:0 rgba(244, 67, 54, 0.85), stop:1 rgba(211, 47, 47, 0.85));
    color: #FFFFFF;
    border: 1px solid rgba(244, 67, 54, 0.4);
    border-radius: 8px;
    padding: 8px 20px;
    font-size: 12px;
    font-weight: 700;
    text-transform: uppercase;
    letter-spacing: 1.2px;
    min-height: 36px;
}

QPushButton#logoutButton:hover {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1,
        stop:0 rgba(244, 67, 54, 1), stop:1 rgba(211, 47, 47, 1));
    border: 1px solid rgba(244, 67, 54, 0.6);
    color: #FFFFFF;
}

QPushButton#logoutButton:pressed {
    background: rgba(183, 28, 28, 0.95);
    border: 1px solid rgba(244, 67, 54, 0.8);
}

/* Window Control Buttons */
QPushButton#minimizeButton,
QPushButton#maximizeButton,
QPushButton#closeButton {
    background: rgba(60, 70, 90, 0.7);
    color: #b0b8c0;
    border: 1px solid rgba(100, 110, 130, 0.4);
    border-radius: 6px;
    padding: 6px 10px;
    font-size: 16px;
    font-weight: 600;
}

QPushButton#minimizeButton:hover,
QPushButton#maximizeButton:hover {
    background: rgba(80, 90, 110, 0.85);
    color: #e0e4e8;
    border: 1px solid rgba(120, 130, 150, 0.6);
}

QPushButton#closeButton:hover {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1,
        stop:0 rgba(244, 67, 54, 0.85), stop:1 rgba(211, 47, 47, 0.85));
    color: #FFFFFF;
    border: 1px solid rgba(244, 67, 54, 0.6);
}

QPushButton#minimizeButton:pressed,
QPushButton#maximizeButton:pressed,
QPushButton#closeButton:pressed {
    background: rgba(40, 50, 70, 0.9);
    border: 1px solid rgba(80, 90, 110, 0.8);
}

/* Connect Mode Button */
QPushButton#connectButton {
    background: rgba(100, 180, 220, 0.15);
    color: #64B4DC;
    border: 1px solid rgba(100, 180, 220, 0.3);
    border-radius: 6px;
    padding: 8px 20px;
    font-size: 12px;
    font-weight: 600;
    text-transform: uppercase;
    letter-spacing: 1px;
    min-height: 32px;
}

QPushButton#connectButton:hover {
    background: rgba(100, 180, 220, 0.25);
}

QPushButton#connectButton:checked {
    background: rgba(100, 180, 220, 0.35);
    border: 1px solid #64B4DC;
    color: #fff;
}

/* Connection Type Combo */
QComboBox#connectionTypeCombo {
    background: rgba(36, 39, 46, 0.9);
    color: #c4c7cc;
    border: 1px solid rgba(255, 255, 255, 0.08);
    border-radius: 6px;
    padding: 6px 14px;
    font-size: 12px;
    min-height: 32px;
}

QComboBox#connectionTypeCombo:hover {
    border: 1px solid rgba(100, 180, 220, 0.3);
}

QComboBox#connectionTypeCombo::drop-down {
    border: none;
}

QComboBox#connectionTypeCombo QAbstractItemView {
    background: #24272e;
    color: #c4c7cc;
    border: 1px solid rgba(255, 255, 255, 0.08);
    selection-background-color: rgba(100, 180, 220, 0.3);
}

/* Voice Alert Buttons */
QPushButton#voiceToggleBtn {
    background: #1b5e20;
    color: #a5d6a7;
    border: 1px solid #2e7d32;
    border-radius: 6px;
    padding: 8px 18px;
    font-size: 12px;
    font-weight: 600;
    text-transform: uppercase;
    letter-spacing: 1px;
    min-height: 32px;
}

QPushButton#voiceToggleBtn:hover {
    background: #2e7d32;
}

QPushButton#voiceToggleBtn:checked {
    background: #1b5e20;
    color: #a5d6a7;
}

QPushButton#voiceToggleBtn:!checked {
    background: #b71c1c;
    color: #ef9a9a;
    border-color: #c62828;
}

QPushButton#testVoiceBtn {
    background: #0d47a1;
    color: #90caf9;
    border: 1px solid #1565c0;
    border-radius: 6px;
    padding: 8px 18px;
    font-size: 12px;
    font-weight: 600;
    text-transform: uppercase;
    letter-spacing: 1px;
    min-height: 32px;
}

QPushButton#testVoiceBtn:hover {
    background: #1565c0;
}

/* Connection Help */
QLabel#connectionHelpLabel {
    color: #5f6368;
    font-size: 11px;
    padding: 8px;
    background: rgba(100, 180, 220, 0.05);
    border-radius: 6px;
    border-left: 2px solid rgba(100, 180, 220, 0.3);
}

/* ============================================
   SCROLLBARS
   ============================================ */

QScrollBar:vertical {
    background: transparent;
    width: 6px;
    border-radius: 3px;
    margin: 2px;
}

QScrollBar::handle:vertical {
    background: rgba(255, 255, 255, 0.1);
    border-radius: 3px;
    min-height: 24px;
}

QScrollBar::handle:vertical:hover {
    background: rgba(255, 255, 255, 0.18);
}

QScrollBar::add-line:vertical, QScrollBar::sub-line:vertical {
    height: 0px;
}

QScrollBar:horizontal {
    background: transparent;
    height: 6px;
    border-radius: 3px;
    margin: 2px;
}

QScrollBar::handle:horizontal {
    background: rgba(255, 255, 255, 0.1);
    border-radius: 3px;
    min-width: 24px;
}

QScrollBar::handle:horizontal:hover {
    background: rgba(255, 255, 255, 0.18);
}

QScrollBar::add-line:horizontal, QScrollBar::sub-line:horizontal {
    width: 0px;
}

/* ============================================
   ROLE SELECTION BUTTONS
   ============================================ */

QPushButton#roleButton {
    background: rgba(45, 49, 59, 0.8);
    color: #c4c7cc;
    border: 1px solid rgba(255, 255, 255, 0.06);
    border-radius: 8px;
    padding: 20px;
    font-size: 15px;
    font-weight: 500;
    text-align: left;
    min-height: 76px;
}

QPushButton#roleButton:hover {
    background: rgba(52, 56, 63, 0.9);
    border: 1px solid rgba(255, 255, 255, 0.1);
}

QPushButton#roleButton:checked {
    background: rgba(0, 188, 212, 0.1);
    border: 1px solid rgba(0, 188, 212, 0.3);
    color: #e8eaed;
}

QPushButton#linkButton {
    background: transparent;
    color: #00BCD4;
    border: none;
    font-size: 14px;
    text-decoration: underline;
    padding: 5px;
}

QPushButton#linkButton:hover {
    color: #4DD0E1;
}

/* ============================================
   TAB WIDGET
   ============================================ */

QTabWidget#componentTabWidget {
    background: transparent;
    border: none;
}

QTabWidget#componentTabWidget::pane {
    background: rgba(18, 20, 26, 0.95);
    border: 1px solid rgba(255, 255, 255, 0.06);
    border-top: none;
    border-radius: 0 0 8px 8px;
}

QTabWidget#componentTabWidget > QTabBar {
    background: transparent;
}

QTabBar::tab {
    background: rgba(36, 39, 46, 0.9);
    color: #9aa0a6;
    border: 1px solid rgba(255, 255, 255, 0.06);
    border-bottom: none;
    padding: 8px 16px;
    margin-right: 2px;
    font-size: 13px;
    font-weight: 600;
    border-radius: 6px 6px 0 0;
    min-width: 80px;
    letter-spacing: 0.5px;
}

QTabBar::tab:hover {
    background: rgba(45, 49, 59, 0.95);
    color: #e8eaed;
    border: 1px solid rgba(0, 188, 212, 0.2);
    border-bottom: none;
}

QTabBar::tab:selected {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1,
        stop:0 rgba(0, 137, 123, 0.9), stop:1 rgba(0, 105, 92, 0.9));
    color: #ffffff;
    border: 1px solid rgba(0, 188, 212, 0.4);
    border-bottom: 2px solid rgba(0, 137, 123, 0.9);
    font-weight: 700;
}

QTabBar::tab:!selected {
    margin-top: 2px;
}

QTabBar::scroller {
    width: 28px;
}

QTabBar QToolButton {
    background: rgba(36, 39, 46, 0.9);
